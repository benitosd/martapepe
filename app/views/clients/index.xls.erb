<h1>Clientes</h1>
<table>
  <tr>
    <th>Nombre</th>
    <th>cliente</th>
    <th>Dirección</th>
    <th>Código postal</th>
    <th>Ciudad</th>
    <th>CIF</th>
  </tr>
  <% @clients_excel.each do |tabular| %>
      <tr>
        <td><%= tabular.name %></td>
        <td><%= tabular.client_number %></td>
        <td><%= tabular.address %></td>
        <td><%= tabular.zip_code %></td>
        <td><%= tabular.city %></td>
        <td><%= tabular.cif %></td>
      </tr>
  <% end %>
</table>